square = lambda x: x*x

print(square(5))

add = lambda a,b: a + b

print(add(2,8))

subtract = lambda a, b: a - b

print(subtract(2,3))

division = lambda a, b: a / b

print(division(8, 2))

modulas = lambda a, b: a % b

print(modulas(7, 2))

points = [(2,3),(1,9),(4,1)]
points.sort(key=lambda x: x[1])
print(points)

nums = [1,2,3,4]
squared = list(map(lambda x: x*x, nums))
print(squared)
